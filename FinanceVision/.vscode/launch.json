    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "JsonFileSaving",
            "request": "launch",
            "mainClass": "persistence/fileSaving.JsonFileSaving",
            "projectName": "persistence"
        },
        {
            "type": "java",
            "name": "FileSaving",
            "request": "launch",
            "mainClass": "persistence/fileSaving.FileSaving",
            "projectName": "persistence"
        },
        {
            "type": "java",
            "name": "FileSaving",
            "request": "launch",
            "mainClass": "app/fileSaving.FileSaving",
            "projectName": "packages-template"
        },
        {
            "type": "java",
            "name": "Account",
            "request": "launch",
            "mainClass": "app/core.Account",
            "projectName": "packages-template"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch packages-template - single-module, multi-package",
            "request": "launch",
            "mainClass": "app/ui.App",
            "vmArgs": [
                "--add-opens",
                "app/core=ALL-UNNAMED",
                "--add-opens",
                "app/ui=ALL-UNNAMED",
                "--add-exports",
                "javafx.graphics/com.sun.javafx.application=ALL-UNNAMED"
            ],
            "projectName": "packages-template"
        }
    ]
}